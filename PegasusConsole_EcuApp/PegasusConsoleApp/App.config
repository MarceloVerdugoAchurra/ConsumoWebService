<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <add name="DBConnection" connectionString="Data Source=DESKTOP-PC;Initial Catalog=DATABASE_NAME;Integrated Security=True"
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="user" value="user@domain.com"/>
    <add key="password" value="passwordExample"/>
    <add key="camposECU" value="value1, value2, value3"/>
